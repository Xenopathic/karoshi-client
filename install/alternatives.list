#Format:
# [alternative name]: [link name] -> [file] [[priority]=100]

#Karoshi utilities
karoshi-set-local-password:        /usr/bin/karoshi-set-local-password         -> /opt/karoshi/linuxclientsetup/utilities/set-local-password
karoshi-set-location:              /usr/bin/karoshi-set-location               -> /opt/karoshi/linuxclientsetup/utilities/set-location
karoshi-set-network:               /usr/bin/karoshi-set-network                -> /opt/karoshi/linuxclientsetup/utilities/set-network
karoshi-setup:                     /usr/bin/karoshi-setup                      -> /opt/karoshi/linuxclientsetup/utilities/setup
karoshi-manage-flags:              /usr/bin/karoshi-manage-flags               -> /opt/karoshi/linuxclientsetup/utilities/manage-flags
karoshi-generate-firewall-rules:   /usr/bin/karoshi-generate-firewall-rules    -> /opt/karoshi/linuxclientsetup/utilities/generate-firewall-rules
karoshi-unset-network:             /usr/bin/karoshi-unset-network              -> /opt/karoshi/linuxclientsetup/utilities/unset-network
karoshi-offline-homes:             /usr/bin/karoshi-offline-homes              -> /opt/karoshi/linuxclientsetup/utilities/offline-homes

#Images
karoshi-login-background:          /usr/share/backgrounds/karoshi-login.png    -> /opt/karoshi/linuxclientsetup/images/login-background.png
karoshi-background:                /usr/share/backgrounds/karoshi.png          -> /opt/karoshi/linuxclientsetup/images/background.png
karoshi-background-nologo:         /usr/share/backgrounds/karoshi-nologo.png   -> /opt/karoshi/linuxclientsetup/images/background-nologo.png
installer-background:              /usr/share/backgrounds/installer.png        -> /usr/share/backgrounds/karoshi-nologo.png

#GRUB
grub-background:                   /boot/grub/background.png                   -> /opt/karoshi/linuxclientsetup/images/boot-menu.png

#Plymouth theme
default.plymouth: /lib/plymouth/themes/default.plymouth -> /lib/plymouth/themes/karoshi-logo/karoshi-logo.plymouth 200
text.plymouth:    /lib/plymouth/themes/text.plymouth    -> /lib/plymouth/themes/karoshi-text/karoshi-text.plymouth 200

#Firefox mozilla.cfg
firefox.mozilla.cfg /usr/lib/firefox/mozilla.cfg -> /var/lib/karoshi/firefox/mozilla.cfg
